{
  "name": "translatorx",
  "version": "2.0.0",
  "license": "MIT",
  "main": "main/index.js",
  "author": {
    "name": "LinHQ"
  },
  "description": "不限篇幅翻译",
  "keywords": [
    "Concurrent",
    "Translate"
  ],
  "repository": {
    "url": "https://github.com/LinHQ1999/TranslatorX"
  },
  "productName": "translateX",
  "scripts": {
    "dev": "concurrently \"yarn dev:vite\" \" yarn dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "npm run build:electron && electron --trace-warnings .",
    "build": "npm run build:vite && npm run build:electron",
    "build:vite": "vite build",
    "build:electron": "tsc -p electron",
    "dist": "npm run build && electron-builder",
    "pack": "npm run build && electron-builder --dir",
    "clean": "rimraf dist main src/out",
    "type-check": "tsc",
    "lint": "eslint . --ext js,jsx,ts,tsx",
    "lint:fix": "eslint . --ext js,jsx,ts,tsx --fix"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.3.0",
    "@fortawesome/free-regular-svg-icons": "^6.0.0",
    "@fortawesome/free-solid-svg-icons": "^6.0.0",
    "@fortawesome/react-fontawesome": "^0.1.17",
    "@tailwindcss/forms": "^0.4.0",
    "@types/crypto-js": "^4.1.0",
    "@vitejs/plugin-react": "^1.2.0",
    "@wordpress/wordcount": "^3.3.1",
    "axios": "^0.25.0",
    "crypto-js": "^4.1.1",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.4.6",
    "electron-store": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "vite": "^2.8.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/electron-store": "^3.2.0",
    "@types/node": "^17.0.17",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/wordpress__wordcount": "^2.4.2",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "5.11.0",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "cssnano": "^5.0.17",
    "electron": "^16.0.7",
    "electron-builder": "^22.14.13",
    "eslint": "8.9.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "postcss": "^8.4.6",
    "prettier": "2.5.1",
    "tailwindcss": "^3.0.22",
    "typescript": "^4.5.5"
  },
  "build": {
    "appId": "cn.linhq.translatorX",
    "asar": true,
    "files": [
      "main",
      "src/out"
    ],
    "win": {
      "target": [
        {
          "target": "dir"
        },
        {
          "target": "portable"
        }
      ],
      "icon": "build/icon.png"
    },
    "directories": {
      "buildResources": "resources"
    }
  }
}
